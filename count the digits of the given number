import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int count=0;
        long num = sc.nextLong();
        while(num>0){
            num=num/10;
            count++;
        }
            System.out.println(count);   
        
    }
}
